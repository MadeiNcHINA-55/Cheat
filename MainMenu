-- Scripts:

local function FRAXWT_fake_script() -- DungeonPlace.LocalScript 
	local script = Instance.new('LocalScript', DungeonPlace)

	local function tp()
		game:GetService('TeleportService'):Teleport(109748636043804)
	end
	
	script.Parent.Activated:Connect(function()
		tp()
	end)
end
coroutine.wrap(FRAXWT_fake_script)()
local function XHDE_fake_script() -- AutoComplete.LocalScript 
	local script = Instance.new('LocalScript', AutoComplete)

	local Player = game.Players.LocalPlayer
	local Char = Player.Character
	local HttpService = game:GetService("HttpService")
	local fileName = "UTD2.txt"
	
	local function readConfig()
		local configString = readfile(fileName)
		return HttpService:JSONDecode(configString)
	end
	
	local function writeConfig(config)
		local configString = HttpService:JSONEncode(config)
		writefile(fileName, configString)
	end
	
	local function updateAutoCompleteEnabled(newValue)
		local config = readConfig()
		config.AutoCompleteEnabled = newValue 
		writeConfig(config)
	
	end
	
	local InAction = false
	
	local function AutoComplete()
	
		if InAction then
			InAction = false
		else
			InAction = true
		end
	
		script.Parent["Y/N"].Text = InAction and "Y" or "N"
		updateAutoCompleteEnabled(InAction)
	
		while InAction == true do
			if not game.Workspace:FindFirstChild("RealDungeons") then
				break
			end
			for _, Dungeon in pairs(game.Workspace.RealDungeons:GetChildren()) do
				if Dungeon.Name ~= game.ReplicatedStorage.ActiveDungeon.Value then return end
				for _, D in pairs(Dungeon.Areas:GetChildren()) do
					for _, Enemy in pairs(D:GetChildren()) do
						if Enemy:IsA("Model") then
							if Enemy:FindFirstChild("HumanoidRootPart") then
								repeat
									game.ReplicatedStorage.UseSpell:FireServer("Toriel' Flame Daggers")
									if Char then
										Char.HumanoidRootPart.CFrame = Enemy.HumanoidRootPart.CFrame * CFrame.new(0,5,2)
									end
									wait(0.25)
								until not Enemy:FindFirstChild("Humanoid") or not Enemy:FindFirstChild("HumanoidRootPart")
							end
						end
					end
				end
				for _, i in pairs(Dungeon:GetChildren()) do
					if i:IsA("Model") then
						if i:FindFirstChild("Humanoid") then
							repeat
								game.ReplicatedStorage.UseSpell:FireServer("Toriel' Flame Daggers")
								if Char then
									Char.HumanoidRootPart.CFrame = i.HumanoidRootPart.CFrame * CFrame.new(0,5,2)
								end
	
								wait(0.25)
							until not i:FindFirstChild("Humanoid") or not i:FindFirstChild("HumanoidRootPart")
						end
					end
				end 
			end
			wait(1)
		end
	end
	
	local config = readConfig()
	
	if config.AutoCompleteEnabled == true then
		AutoComplete()
	end
	
	
	script.Parent.Activated:Connect(function()
		AutoComplete()
	end)
end
coroutine.wrap(XHDE_fake_script)()
local function FZXJZYV_fake_script() -- Leave.LocalScript 
	local script = Instance.new('LocalScript', Leave)

	script.Parent.Activated:Connect(function()
		script.Parent.Parent.Main.Visible = not script.Parent.Parent.Main.Visible
	end)
end
coroutine.wrap(FZXJZYV_fake_script)()
